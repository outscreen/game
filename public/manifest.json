{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },

  "background": {
    "scripts": [
      "build/background.js"
    ]
  },

  "permissions": [
    "activeTab",
    "notifications",
    "https://ajax.googleapis.com/",
    "http://127.0.0.1/"
  ]
}